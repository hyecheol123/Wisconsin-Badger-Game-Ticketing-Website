    "deploy": "firebase deploy",
    "predeploy": "npm run build"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css}": [
      "npm run lint:fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hyecheol123/Wisconsin-Badger-Game-Ticketing-Website.git"
